{
  "name": "@freon4dsl/core",
  "version": "0.7.0-beta",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/freon4dsl/Freon4dsl"
  },
  "scripts": {
