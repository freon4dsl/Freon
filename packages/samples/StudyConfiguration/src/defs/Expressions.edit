editor default

NumberLiteralExpression {
    [${value}]
}
StartDay {
    [Start Day ]
}

LastDay {
    [Last Day ]
}

PreviousVisit {
    [Previous-Visit ]
}

NextVisit {
    [Next-Visit ]
}

PlusExpression {
    symbol = "+"
}

MinusExpression {
    symbol = "-"
}

Number {
    [${value}]
}

String {
    ['${value}']
    trigger = "'"
}

BooleanLiteralExpression {
    [ ${value} ]
    trigger = "boolean"
}

MultiplyExpression {
    symbol = "*"
}

DivideExpression {
    symbol = "/"
}

LessThenExpression {
    symbol = "<"
}

OrExpression {
    symbol = "or"
}

AndExpression {
    symbol = "and"
}

GreaterThenExpression {
    symbol = ">"
}
EqualsExpression {
    symbol = "=="
}

IfExpression {
        [
        if (${condition}) then
            ${whenTrue}
        else
            ${whenFalse}
        endif
        ]
    trigger = "if"
}

