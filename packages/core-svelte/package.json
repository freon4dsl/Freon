{
  "publishConfig": {
    "access": "public"
  },
  "name": "@freon4dsl/core-svelte",
  "version": "0.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/freon4dsl/Freon4dsl"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/",
    "build": "rollup -c",
    "build-release": "npm run build",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "install-local": "npm install --registry http://localhost:4873",
    "republish-local": "npm run unpublish-local && npm publish-local",
    "release": "npm publish",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873",
    "test": "jest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "3.1.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^27.4.0",
    "jest-transform-stub": "^2.0.0",
    "svelte-jester": "^2.3.1",
    "ts-jest": "^27.1.3"
  },
  "dependencies": {
    "@freon4dsl/core": "0.6.0",
    "mobx": "6.3.3",
    "tslib": "2.4.0"
  }
}
