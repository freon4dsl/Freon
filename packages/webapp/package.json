{
  "name": "@freon4dsl/webapp",
  "version": "0.7.0-beta",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/",
    "build": "echo No build for webapp",
    "build-app": "rollup -c",
    "dev": "cross-env NODE_PORT=8001 rollup -c -w",
    "start": "sirv public --port 8000",
    "validate": "svelte-check --tsconfig ./tsconfig.json",
    "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "install-local": "npm install --registry http://localhost:4873",
    "prepare-app": "npm run styles-light && npm run styles-dark",
    "styles-light": "smui-theme compile public/site.css -i src/webapp/lib/style",
    "styles-dark": "smui-theme compile public/site-dark.css -i src/webapp/lib/style/dark"
  },
  "devDependencies": {
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.4.47",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@smui-extra/autocomplete": "^7.0.0",
    "@smui/banner": "^7.0.0",
    "@smui/button": "^7.0.0",
    "@smui/card": "^7.0.0",
    "@smui/checkbox": "^7.0.0",
    "@smui/common": "^7.0.0",
    "@smui/data-table": "^7.0.0",
    "@smui/dialog": "^7.0.0",
    "@smui/drawer": "^7.0.0",
    "@smui/form-field": "^7.0.0",
    "@smui/icon-button": "^7.0.0",
    "@smui/layout-grid": "^7.0.0",
    "@smui/linear-progress": "^7.0.0",
    "@smui/list": "^7.0.0",
    "@smui/menu": "^7.0.0",
    "@smui/menu-surface": "^7.0.0",
    "@smui/paper": "^7.0.0",
    "@smui/radio": "^7.0.0",
    "@smui/tab": "^7.0.0",
    "@smui/tab-bar": "^7.0.0",
    "@smui/textfield": "^7.0.0",
    "@smui/tooltip": "^7.0.0",
    "@smui/top-app-bar": "^7.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "rollup": "^4.18.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "smui-theme": "^7.0.0",
    "svelte-check": "^3.8.0",
    "svelte-preprocess": "^5.1.4",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@freon4dsl/core": "0.7.0-beta",
    "@freon4dsl/core-svelte": "0.7.0-beta",
    "@freon4dsl/server": "0.6.0",
    "@freon4dsl/samples-example": "0.6.0",
    "@types/node": "^20.14.1",
    "click-outside": "^2.0.2",
    "kotlin": "^1.5.21",
    "lodash": "^4.17.21",
    "mobx": "^6.12.3",
    "mobx-utils": "^6.0.8",
    "net.akehurst.language-agl-processor": "4.0.0-rc.5",
    "sirv-cli": "^2.0.2",
    "tslib": "^2.6.2"
  }
}
