{
  "private": true,
  "name": "@projectit/playground",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "scripts": {
    "projectIt": 	"ts-node ../meta/src/commandline/projectit -v generate-all -d src/demo/defs -o src/demo",
    "projectIt-nonVerbose": 	"ts-node ../meta/src/commandline/projectit generate-all -d src/demo/defs -o src/demo",
    "metaIt": 		"ts-node ../meta/src/commandline/projectit -v generate-language -l src/demo/defs/LanguageDefinition.lang -o src/demo",
    "scopeIt": 		"ts-node ../meta/src/commandline/projectit -v generate-scoper -l src/demo/defs/LanguageDefinition.lang -s src/demo/defs/LanguageDefinition.scop -o src/demo",
    "editIt": 		"ts-node ../meta/src/commandline/projectit -v generate-editor -l src/demo/defs/LanguageDefinition.lang -e src/demo/defs/LanguageDefinition.edit -o src/demo",   
    "validateIt": 	"ts-node ../meta/src/commandline/projectit -v generate-validator -l src/demo/defs/LanguageDefinition.lang -c src/demo/defs/LanguageDefinition.valid -o src/demo",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "build-wait": "tsc -w",
    "build": "yarn run projectIt-nonVerbose && tsc ",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873",
    "test": "jest"
  },
  "devDependencies": {
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router": "^5.1.4",
    "@types/webpack": "^4.39.2",
    "@types/webpack-env": "^1.14.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "@projectit/core": "^0.0.5",
    "@projectit/meta": "^0.0.5",
    "@types/lodash": "4.14.73",
    "@types/pegjs": "^0.10.1",
    "@types/history": "^4.7.5",
    "global": "^4.4.0",
    "prettier": "^1.15.3",
    "typescript": "^3.8.3",
    "mobx": "^5.14.0",
    "mobx-react": "6.1.3"
  },
  "publishConfig": {
    "access": "private"
  }
}
